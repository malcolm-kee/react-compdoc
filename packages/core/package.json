{
  "name": "@showroomjs/core",
  "version": "2.1.1",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist-esm/index.js",
  "types": "dist-esm/index.d.ts",
  "scripts": {
    "build": "pnpm build:cjs && pnpm build:esm",
    "build:cjs": "tsc -b .",
    "build:esm": "tsc --project tsconfig.esm.json",
    "dev": "concurrently \"pnpm dev:cjs\" \"pnpm dev:esm\"",
    "dev:cjs": "pnpm build:cjs --watch",
    "dev:esm": "pnpm build:esm --watch",
    "clean": "rimraf dist dist-esm *.tsbuildinfo",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --watchAll=false --coverage --runInBand"
  },
  "devDependencies": {
    "axe-core": "^4.4.3",
    "esbuild": "0.14.53",
    "html-webpack-tags-plugin": "^3.0.2",
    "react-docgen-typescript": "^2.2.2",
    "typescript": "~4.6.4",
    "webpack": "^5.74.0"
  },
  "peerDependencies": {
    "axe-core": "*",
    "esbuild": "^0.14.0",
    "webpack": "^5.0.0"
  },
  "dependencies": {
    "acorn": "^8.8.0",
    "acorn-walk": "^8.2.0",
    "domutils": "^3.0.1",
    "htmlparser2": "^8.0.1",
    "prism-react-renderer": "^1.3.5",
    "tslib": "^2.4.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
